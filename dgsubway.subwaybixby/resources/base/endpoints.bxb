endpoints {
  action-endpoints {
    action-endpoint (SubwayOne) {
      accepted-inputs (subwayName)
      local-endpoint (subwayOne.js)
    }
    action-endpoint (SubwayOne_ForJunctionStations){
      accepted-inputs (junctionStations, subwayLine)
      local-endpoint (subwayOne_ForJunctionStations.js)
    }
    action-endpoint (SubwayOne_ForLastStations){
      accepted-inputs (subwayDir)
      local-endpoint (subwayOne_ForLastStations.js)
    }
    action-endpoint(SubwayTwo){
      accepted-inputs (subwayName, subwayDir)
      local-endpoint (subwayTwo.js)
    }
    action-endpoint (SubwayTwo_ForJunctionStations){
      accepted-inputs (junctionStations, subwayDir, subwayLine)
      local-endpoint (subwayTwo_ForJunctionStations.js)
    }
    action-endpoint(SubwayThree){
      accepted-inputs (subwayName, isFirstOrLast)
      local-endpoint (subwayThree.js)
    }
    action-endpoint (SubwayThree_ForLastStations){
      accepted-inputs (subwayDir, isFirstOrLast)
      local-endpoint (subwayThree_ForLastStations.js)
    }
    action-endpoint(SubwayFour){
      accepted-inputs (subwayName, subwayDir, isFirstOrLast)
      local-endpoint (subwayFour.js)
    }
  }
}