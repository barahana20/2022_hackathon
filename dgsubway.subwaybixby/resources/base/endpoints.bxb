endpoints {
  action-endpoints {
    action-endpoint (SubwayOne) {
      accepted-inputs (subwayName)
      local-endpoint (subwayOne.js)
    }
    action-endpoint(SubwayTwo){
      accepted-inputs (subwayName, subwayDir)
      local-endpoint (subwayTwo.js)
    }
    action-endpoint (SubwayThree){
      accepted-inputs (subwayName,subwayFirstTime)
      local-endpoint (subwayThree.js)
    }
  }
}
